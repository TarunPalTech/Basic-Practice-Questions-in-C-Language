5.    1
      12
      123	
      1234
      12345
#include<stdio.h>
int main(){
    int i,j;
    for(i=1;i<=5;i++){
        for(j=1;j<=5;j++){
            if(i>=j)
                printf("%d",j);
            else
                printf(" ");
        }
        printf("\n");
    }
}
6.    1
     21
    321
   4321
#include<stdio.h>
int main(){
    int i,j;
    for(i=1;i<=4;i++){
        int flag=i;
        for(j=1;j<=4;j++){
            if(i>=j){
                printf("%d",flag);
                flag--;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}
7.       ABCDE
          ABCD
           ABC
            AB
             A
#include<stdio.h>
int main(){
    int i,j;
    for(i=1;i<=5;i++){
        char flag='A';
        for(j=1;j<=5;j++){
            if(j<=5-i){
                printf(" ");
            }
            else{
                printf("%c",flag);
                flag++;
            }
        }
        printf("\n");
    }
}
8.   1
     23
     456
     78910
#include<stdio.h>
int main(){
    int i,j,flag=1;
    for(i=1;i<=4;i++){
        for(j=1;j<=4;j++){
            if(j>=5-i){
                printf("%d",flag);
                flag++;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}




9.       ABCDE
          BCDE
           CDE
            DE
             E
#include<stdio.h>
int main(){
    int i,j;
    for(i=1;i<=5;i++){
        //char flag=64;
        for(j=1;j<=5;j++){
            if(j>=i){
                printf("%c",64+j);
		  //flag++;
            }
            else{
                printf(" ");
                //flag++;
        }
        printf("\n");
    }
}
10.    *****
       *   *
       *   *
       *   *
       *****
#include<stdio.h>
int main(){
    int i,j;
    for(i=1;i<=5;i++){
        for(j=1;j<=5;j++){
            if((i==1 || j==1) || (i==5 || j==5)){
                printf("*");
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}
